{
  "manifest_version": 2,
  "name": "Dev Accounts",
  "description": "Easily access your developer-related accounts!",
  "version": "1.0.0",
  "icons": {
    "128": "DevAccountsLogo-128px.png"
  },
  "browser_action": {
    "default_icon": "DevAccountsLogo-24px.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}